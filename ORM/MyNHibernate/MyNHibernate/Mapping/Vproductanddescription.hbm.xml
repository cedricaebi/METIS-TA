<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="MyNHibernate" namespace="MyNHibernate.Domain" xmlns="urn:nhibernate-mapping-2.2">
  <class name="Vproductanddescription" table="vProductAndDescription" lazy="true" >
    <property name="Productid">
      <column name="ProductID" sql-type="int" not-null="true" />
    </property>
    <property name="Name">
      <column name="Name" sql-type="nvarchar" not-null="true" />
    </property>
    <property name="Productmodel">
      <column name="ProductModel" sql-type="nvarchar" not-null="true" />
    </property>
    <property name="Cultureid">
      <column name="CultureID" sql-type="nchar" not-null="true" />
    </property>
    <property name="Description">
      <column name="Description" sql-type="nvarchar" not-null="true" />
    </property>
  </class>
</hibernate-mapping>