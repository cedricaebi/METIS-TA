<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="MyNHibernate" namespace="MyNHibernate.Domain" xmlns="urn:nhibernate-mapping-2.2">
  <class name="Vproductmodelinstructions" table="vProductModelInstructions" lazy="true" >
    <property name="Productmodelid">
      <column name="ProductModelID" sql-type="int" not-null="true" />
    </property>
    <property name="Name">
      <column name="Name" sql-type="nvarchar" not-null="true" />
    </property>
    <property name="Instructions">
      <column name="Instructions" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="Locationid">
      <column name="LocationID" sql-type="int" not-null="false" />
    </property>
    <property name="Setuphours">
      <column name="SetupHours" sql-type="decimal" not-null="false" />
    </property>
    <property name="Machinehours">
      <column name="MachineHours" sql-type="decimal" not-null="false" />
    </property>
    <property name="Laborhours">
      <column name="LaborHours" sql-type="decimal" not-null="false" />
    </property>
    <property name="Lotsize">
      <column name="LotSize" sql-type="int" not-null="false" />
    </property>
    <property name="Step">
      <column name="Step" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="Rowguid">
      <column name="rowguid" sql-type="uniqueidentifier" not-null="true" />
    </property>
    <property name="Modifieddate">
      <column name="ModifiedDate" sql-type="datetime" not-null="true" />
    </property>
  </class>
</hibernate-mapping>