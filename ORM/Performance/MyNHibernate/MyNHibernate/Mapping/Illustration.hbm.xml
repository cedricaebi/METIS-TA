<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="MyNHibernate" namespace="MyNHibernate.Domain" xmlns="urn:nhibernate-mapping-2.2">
  <class name="Illustration" table="Illustration" lazy="true" >
    <id name="Illustrationid" column="IllustrationID">
      <generator class="identity" />
    </id>
    <property name="Diagram">
      <column name="Diagram" sql-type="xml" not-null="false" />
    </property>
    <property name="Modifieddate">
      <column name="ModifiedDate" sql-type="datetime" not-null="true" />
    </property>
  </class>
</hibernate-mapping>