<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="MyNHibernate" namespace="MyNHibernate.Domain.SQLite" xmlns="urn:nhibernate-mapping-2.2">
  <class name="Album" table="Album" lazy="true" >
    <id name="Albumid" column="AlbumId" />
    <property name="Title">
      <column name="Title" sql-type="NVARCHAR" not-null="true" />
    </property>
    <property name="Artistid">
      <column name="ArtistId" sql-type="INTEGER" not-null="true" />
    </property>
  </class>
</hibernate-mapping>