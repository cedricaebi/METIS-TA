<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="MyNHibernate" namespace="MyNHibernate.Domain" xmlns="urn:nhibernate-mapping-2.2">
  <class name="Vstorewithaddresses" table="vStoreWithAddresses" lazy="true" >
    <property name="Businessentityid">
      <column name="BusinessEntityID" sql-type="int" not-null="true" />
    </property>
    <property name="Name">
      <column name="Name" sql-type="nvarchar" not-null="true" />
    </property>
    <property name="Addresstype">
      <column name="AddressType" sql-type="nvarchar" not-null="true" />
    </property>
    <property name="Addressline1">
      <column name="AddressLine1" sql-type="nvarchar" not-null="true" />
    </property>
    <property name="Addressline2">
      <column name="AddressLine2" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="City">
      <column name="City" sql-type="nvarchar" not-null="true" />
    </property>
    <property name="Stateprovincename">
      <column name="StateProvinceName" sql-type="nvarchar" not-null="true" />
    </property>
    <property name="Postalcode">
      <column name="PostalCode" sql-type="nvarchar" not-null="true" />
    </property>
    <property name="Countryregionname">
      <column name="CountryRegionName" sql-type="nvarchar" not-null="true" />
    </property>
  </class>
</hibernate-mapping>