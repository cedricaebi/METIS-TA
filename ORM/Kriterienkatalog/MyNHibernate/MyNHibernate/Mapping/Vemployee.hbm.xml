<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="MyNHibernate" namespace="MyNHibernate.Domain" xmlns="urn:nhibernate-mapping-2.2">
  <class name="Vemployee" table="vEmployee" lazy="true" >
    <property name="Businessentityid">
      <column name="BusinessEntityID" sql-type="int" not-null="true" />
    </property>
    <property name="Title">
      <column name="Title" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="Firstname">
      <column name="FirstName" sql-type="nvarchar" not-null="true" />
    </property>
    <property name="Middlename">
      <column name="MiddleName" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="Lastname">
      <column name="LastName" sql-type="nvarchar" not-null="true" />
    </property>
    <property name="Suffix">
      <column name="Suffix" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="Jobtitle">
      <column name="JobTitle" sql-type="nvarchar" not-null="true" />
    </property>
    <property name="Phonenumber">
      <column name="PhoneNumber" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="Phonenumbertype">
      <column name="PhoneNumberType" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="Emailaddress">
      <column name="EmailAddress" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="Emailpromotion">
      <column name="EmailPromotion" sql-type="int" not-null="true" />
    </property>
    <property name="Addressline1">
      <column name="AddressLine1" sql-type="nvarchar" not-null="true" />
    </property>
    <property name="Addressline2">
      <column name="AddressLine2" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="City">
      <column name="City" sql-type="nvarchar" not-null="true" />
    </property>
    <property name="Stateprovincename">
      <column name="StateProvinceName" sql-type="nvarchar" not-null="true" />
    </property>
    <property name="Postalcode">
      <column name="PostalCode" sql-type="nvarchar" not-null="true" />
    </property>
    <property name="Countryregionname">
      <column name="CountryRegionName" sql-type="nvarchar" not-null="true" />
    </property>
    <property name="Additionalcontactinfo">
      <column name="AdditionalContactInfo" sql-type="xml" not-null="false" />
    </property>
  </class>
</hibernate-mapping>