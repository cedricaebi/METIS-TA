<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="MyNHibernate" namespace="MyNHibernate.Domain" xmlns="urn:nhibernate-mapping-2.2">
  <class name="Productcategory" table="ProductCategory" lazy="true" >
    <id name="Productcategoryid" column="ProductCategoryID">
      <generator class="identity" />
    </id>
    <property name="Name">
      <column name="Name" sql-type="nvarchar" not-null="true" />
    </property>
    <property name="Rowguid">
      <column name="rowguid" sql-type="uniqueidentifier" not-null="true" />
    </property>
    <property name="Modifieddate">
      <column name="ModifiedDate" sql-type="datetime" not-null="true" />
    </property>
    <set name="Productsubcategory">
      <key column="ProductCategoryID" />
      <one-to-many class="Productsubcategory" />
    </set>
  </class>
</hibernate-mapping>